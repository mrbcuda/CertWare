               id="net.certware.argument.arm.section.ArgumentReasoning"
               tab="Base">
             <input
                    type="net.certware.argument.arm.ArgumentReasoning">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.argument.arm.section.CitationElement"
               tab="Base">
