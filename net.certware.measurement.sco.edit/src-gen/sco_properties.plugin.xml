<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://www.certware.net/sco"
            class="net.certware.measurement.sco.providers.ScoEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="net.certware.measurement.sco.providers.ScoPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

   <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="net.certware.measurement.sco.properties">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="net.certware.measurement.sco.properties">
         <propertyTab
               label="Base"
               category="default"
               id="Base">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="net.certware.measurement.sco.properties">
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.TotalChangeOrders"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.TotalChangeOrders">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.CriticalDefectChangeOrders"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.CriticalDefectChangeOrders">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.NormalDefectChangeOrders"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.NormalDefectChangeOrders">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.ImprovementChangeOrders"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.ImprovementChangeOrders">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.NewFeatureChangeOrders"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.NewFeatureChangeOrders">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.ArtifactIdentifier"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.ArtifactIdentifier">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.ArtifactCommit"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.ArtifactCommit">
             </input>
         </propertySection>
         <propertySection
               class="org.eclipse.emf.eef.runtime.ui.properties.sections.PropertiesEditionSection"
               id="net.certware.measurement.sco.section.CommitHistory"
               tab="Base">
             <input
                    type="net.certware.measurement.sco.CommitHistory">
             </input>
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
